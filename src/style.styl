body
    display flex
    justify-content center
    align-items center
    // background-color rgb(45, 45, 45)
    background-color rgb(20, 20, 20)
    margin 0
    position absolute
    width 100%
    height 100%

.wrapper
    position absolute
    // width 440px
    // height 440px
    
    display flex
    justify-content center
    align-items: center
    background linear-gradient(to bottom, #005500, #000000)
    border-radius 4px
    // width 60vmin
    // height 100vmin
    @media screen and (max-aspect-ratio: 3/5)
        width 0.9 * 100vw
        height 0.9 * 166vw
    @media screen and (min-aspect-ratio: 3/5) 
        height 0.9 * 100vh
        width 0.9 * 60vh
    



    
.canvas
    // background-color red
    // transform-origin top left
    // transform scale(6)
    border 4px solid #32CE36
    box-shadow 0px 0px 16px 0px black
    position relative
    width 90%
    height 90%
    